{% extends 'base.html.twig' %}

{% block title %}{{ title }}{% endblock %}

{% block breadcrumb %}
	{{ parent() }}
	<li class="nav-item">
		<a class="nav-link active" href="#">{{ title }}</a>
	</li>
{% endblock %}

{% block body %}
	<div class="container">
		<h1 class="text-center mt-4">{{ title }}</h1>
		<div class="row justify-content-md-center">
			<div class="col-md-6">
				{{ form(attachmentForm) }}
            </div>
        </div>
    </div>
{% endblock %}
